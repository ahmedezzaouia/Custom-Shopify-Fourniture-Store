    font-weight: 800;
  }

  .hero-tag {
    display: inline-block;
    font-size: .5em;
    padding: .5em;
    background: #fff;
    color: #000;
  }

  .container-hero h2,
  .container-hero p {
    margin: 0;
  }

  .container-hero p {
    letter-spacing: 1px;
