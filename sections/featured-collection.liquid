<!-- this for Features Products  -->
<div class="shell">
  <div class="container">
    <div class="container text-center">
      <h1>Best Products</h1>
    </div>

    <div class="row">
      {% for product in collections.collection-1.products %}
      <div class="col-md-3">
        <div class="wsk-cp-product">
          <div class="wsk-cp-img">
            <a href="{{product.url}}">

              <img src="{{product.featured_media |  img_url : '500x500'}}" alt="Product" class="img-responsive" />
            </a>
          </div>
          <div class="wsk-cp-text">
            <div class="category">

              <span>Retals</span>

            </div>
            <div class="title-product">
              <h3>{{product.title}}</h3>
            </div>
            <div class="description-prod">
              <p>{{product.content}}</p>
            </div>
            <div class="card-footer">
              <div class="wcf-left"><span class="price">{{product.price | money_with_currency}}</span></div>
            </div>
          </div>
        </div>
      </div>
      {% endfor %}



    </div>
    <div class="row">
      <div class="col-md-6">
        <div class="wsk-cp-product">
          <div class="wsk-cp-img">
            <a href="https://n9mpxwdfa26jifwt-58504773827.shopifypreview.com/products/notes-tree">

              <img src="https://cdn.shopify.com/s/files/1/0585/0477/3827/products/shop-img51_550x.jpg?v=1642339182"
                alt="Product" class="img-responsive" />
            </a>
          </div>
          <div class="wsk-cp-text">
            <div class="category">
              <span>Retals</span>
            </div>
            <div class="title-product">
              <h3>Notes Tree</h3>
            </div>
            <div class="description-prod">
              <p>Description Product tell me how to change playlist height size like 600px in html5 player. player good
                work now check this link</p>
            </div>
            <div class="card-footer">
              <div class="wcf-left"><span class="price">$28.00 USD</span></div>
            </div>
          </div>
        </div>
      </div>
      {% for product in collections.collection-2.products %}
      <div class="col-md-3">
        <div class="wsk-cp-product">
          <div class="wsk-cp-img">
            <a href="{{product.url}}">

              <img src="{{product.featured_media |  img_url : '500x500'}}" alt="Product" class="img-responsive" />
            </a>
          </div>
          <div class="wsk-cp-text">
            <div class="category">

              <span>Retals</span>

            </div>
            <div class="title-product">
              <h3>{{product.title}}</h3>
            </div>
            <div class="description-prod">
              <p>{{product.content}}</p>
            </div>
            <div class="card-footer">
              <div class="wcf-left"><span class="price">{{product.price | money_with_currency}}</span></div>
            </div>
          </div>
        </div>
      </div>

      {% endfor %}


    </div>
  </div>

  <figure class="container-hero effect-move">
    <img class="container-hero-image effect-image"
      src="https://cdn.pixabay.com/photo/2015/12/05/23/16/office-1078869_960_720.jpg" alt="" />
    <figcaption class="container-hero-content">
      <span class="hero-tag effect-target">Category</span>
      <h2 class="container-hero-title effect-target">Another Long Title With Lots of Text</h2>
      <p class="hero-text effect-target effect-text">This description will have to be truncated if it becomes too
        long.</p>
      <a class="container-hero-link" href="">View more</a>
    </figcaption>
  </figure>


</div>
<!-- End for Features Products  -->








<style>
